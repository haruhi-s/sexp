{
    "name": "sexp",
    "displayName": "sexp",
    "description": "sexp in vscode",
    "version": "0.1.2",
    "publisher": "haruhi-s",
    "homepage": "https://github.com/haruhi-s/sexp",
	"repository": {
		"type": "git",
		"url": "https://github.com/haruhi-s/sexp.git"
	},
	"bugs": "https://github.com/haruhi-s/sexp/issues",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "sexp.slurp",
                "title": "slurp forward"
            }
        ],
        "keybindings": [
            {
                "command": "sexp.slurp",
                "key": "alt+k"
            },
            {
                "command": "sexp.moveForward",
                "key": "alt+l"
            },
            {
                "command": "sexp.moveBackward",
                "key": "alt+j"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}